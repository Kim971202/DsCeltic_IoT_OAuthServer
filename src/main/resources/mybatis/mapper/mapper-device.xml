<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.oauth.mapper.DeviceMapper">

    <resultMap id="device" type="com.oauth.dto.authServerDTO"> </resultMap>

    <select id="getSerialNumberBydeviceId" parameterType="String" resultMap="device">
        SELECT SERIAL_NO AS serialNumber
        FROM TBR_IOT_DEVICE
        WHERE DEVC_ID = #{deviceId}
    </select>


</mapper>